{
  "package": "com.memo.demo",
  "name": "L1uZQ_Todo_demo",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1070,
  "icon": "/assets/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "service.share"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.vibrator"
    },
    {
      "name": "service.asr"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "memo",
  "config": {
    "logLevel": "debug",
    "designWidth": 360
  },
  "router": {
    "entry": "pages/main",
    "pages": {
      "pages/main": {
        "component": "index"
      },
      "pages/add/add_note": {
        "component": "index"
      },
      "pages/add/add_todo": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBar": true,
    "titleBarBackgroundColor": "#f4f6fa",
    "titleBarTextColor": "#333333",
    "menu": true,
    "pages": {
      "pages/main": {
        "titleBarText": "快简记"
      },
      "pages/add/add_todo": {
        "titleBarText": "添加待办"
      },
      "pages/add/add_note": {
        "titleBarText": "添加笔记"
      }
    }
  }
}
